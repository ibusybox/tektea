<h4>总共<%= metaList.length %>篇文章</h4>
<table class="table table-hover">
    <thead>
        <tr>
            <th>标题</th>
            <th>作者</th>
            <th>类别</th>
            <th>阅读次数</th>
        </tr>
    </thead>
    <tbody>
        <% for ( var i = 0; i < metaList.length; i++){%>
            <tr>
                <td><a href="/article/?title=<%= metaList[i].title %>&writer=<%= metaList[i].writer %>"><%= metaList[i].title %></a></td>
                <td><a href="/writer/?name=<%= metaList[i].writer %>"><%= metaList[i].writer %></a></td>
                <td><%= metaList[i].category %></td>
                <td><%= metaList[i].viewCount %></td>
            </tr>
        <%}%>
    </tbody>
</table>